  
{
    "name": "jQuery Injector",
    "version": "1.0",
    "description": "Inject jQuery 3.2.1 on any page. Quick and easy.",
    "permissions": [
      "activeTab"
    ],
    "icons": {
     "128": "icon128.png" },
    "browser_action": {
        "default_title": "Jquery Injector",
        "default_icon": "icon128.png",
        "default_popup": "screen1.html"
    },
    "web_accessible_resources": [
      "jquery.js",
      "app.js"
    ],
    "manifest_version": 2,
    "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
  }